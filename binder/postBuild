pip install arviz
pip install numdifftools
pip install https://storage.googleapis.com/tensorflow/linux/cpu/tensorflow-1.8.0-cp36-cp36m-linux_x86_64.whl
pip install keras
pip install https://robot.inf.um.es/material/umucv.tar.gz

jupyter contrib nbextension install --user
jupyter nbextension enable --py widgetsnbextension
jupyter nbextension enable python-markdown/main

# Notebooks w/ extensions that auto-run code must be "trusted" to work the first time
# jupyter trust notebooks/matplotlib.ipynb

